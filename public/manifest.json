{
  "manifest_version": 3,
  "name": "Wingman",
  "version": "1.0.0",
  "description": "Your flight search assistant.",
  "action": {
    "default_popup": "index.html",
    "default_title": "Wingman",
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}